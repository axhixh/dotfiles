alias vi=nvim
alias vim=nvim
alias ls="exa"
alias ll="exa -hl"
alias lr="exa -hl -s changed"
alias ..='cd ..'
alias ...='cd ../..'
alias df='df -h'
alias du='du -h -c'
alias du1='du -h -c -d 1'
alias mkdir='mkdir -p -v'
alias c='clear'
alias cat=bat
alias g='./gradlew build'
alias k='kubectl'
alias incoming='tig ..origin/main'
alias dci="docker images | grep none | awk '{print $3}' | xargs docker rmi"
alias gbd='git branch -D $(git branch | fzf)'
alias gm=git-machete
