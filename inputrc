set editing-mode vi
# additinally use jj for esc
#"jj": vi-movement-mode
# beginning and end of history
#"gg": beginning-of-history
#"G": end-of-history
# no visible or audible alerts
set bell-style none
# just show the options if more than one
set show-all-if-ambiguous On
# possible completion coloured based on file type
set colored-stats On
