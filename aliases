alias ..='cd ..'
alias ...='cd ../..'
alias c='clear'
alias cat='bat --theme ansi'
alias dci="docker images | grep none | awk '{print $3}' | xargs docker rmi"
alias df='df -h'
alias du='du -h -c'
alias du1='du -h -c -d 1'
alias g='./gradlew build'
alias gbd='git branch -D $(git branch | fzf)'
alias gm=git-machete
alias icat='kitty icat'
alias incoming='tig ..origin/main'
alias k='kubectl'
alias lg="eza -hl --git"
alias ll="eza -hl"
alias lr="eza -hl --git -s changed"
alias ls="eza"
alias mkdir='mkdir -p -v'
alias tree="eza -h --tree"
alias vi=hx
